# Upstash Redis (for BullMQ)
UPSTASH_REDIS_URL=redis://default:xxx@xxx.upstash.io:6379
# REDIS_URL=... # alternative to UPSTASH_REDIS_URL

# Supabase
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...
# For migrations (scripts/run-migration.mjs)
SUPABASE_PROJECT_REF=your-project-ref
SUPABASE_DB_PASSWORD=your-db-password

# Replicate
REPLICATE_API_TOKEN=r8_xxx

# OpenAI (for generate-content)
OPENAI_API_KEY=sk-xxx

# Maton (for Save to Drive - Google Drive)
# Get key from https://maton.ai/api-keys
MATON_API_KEY=your-maton-api-key
# Optional: folder ID for nablinds/content (file created in Drive root if not set)
# MATON_DRIVE_FOLDER_ID=

# Server
PORT=3000
