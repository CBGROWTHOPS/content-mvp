{
  "name": "content-mvp",
  "version": "0.1.0",
  "description": "Automated content generation system",
  "type": "module",
  "main": "dist/api/index.js",
  "scripts": {
    "db:migrate": "node scripts/run-migration.mjs",
    "db:create-bucket": "node scripts/create-storage-bucket.mjs",
    "dev": "tsx watch src/api/index.ts",
    "dev:worker": "tsx watch src/worker/index.ts",
    "build": "tsc && cp -r brands dist/brands",
    "start": "node dist/api/index.js",
    "worker": "node dist/worker/index.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "bullmq": "^5.25.0",
    "cors": "^2.8.6",
    "express": "^4.21.0",
    "ioredis": "^5.4.1",
    "replicate": "^0.34.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.0",
    "@types/pg": "^8.11.0",
    "pg": "^8.13.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18"
  }
}
